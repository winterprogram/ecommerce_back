{
  "id": "/drawer",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "drawer widget",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "name",
    "merchantId",
    "sub_section"
  ],
  "properties": {
    "name": {
      "type": "string",
      "description": "Name of main drawer loder"
    },
    "merchantId": {
      "type": "string"
    },
    "sub_section": {
      "type": "boolean"
    },
    "sub": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          }
        }
      }
    }
  },
  "is_active": {
    "type": "boolean"
  },
  "created_at": {
    "type": "string"
  },
  "updated_at": {
    "type": "string"
  }
}